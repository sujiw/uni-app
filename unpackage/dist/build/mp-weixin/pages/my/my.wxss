
.user_info {
	text-align: center;
	color: #FFFFFF;
	height: 300rpx;
}
.user_info .head {
	width: 100%;
	padding: 20rpx;
}
.user_info .head image {
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	border: 1rpx solid #f5f5f5;
	height: 150rpx;
	background: #FFFFFF;
	width: 150rpx;
}
.user_info .info {
	font-size: 30rpx;
	line-height: 80rpx;
}

